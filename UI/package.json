{
  "name": "vynce-standalone",
  "version": "1.0.0",
  "description": "Vynce Image Search Desktop Application",
  "author": "Vynce",
  "private": true,
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "electron .",
    "build:angular": "ng build --configuration production",
    "build": "npm run build:angular && electron-builder",
    "watch": "nodemon --exec electron .",
    "test": "ng test"
  },
  "dependencies": {
    "@angular/animations": "^21.0.6",
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@auth0/angular-jwt": "^5.2.0",
    "@chroma-core/default-embed": "^0.1.9",
    "@microsoft/applicationinsights-web": "^3.3.10",
    "@ngx-translate/core": "^17.0.0",
    "@ngx-translate/http-loader": "^17.0.0",
    "@primeuix/themes": "^2.0.2",
    "crypto-js": "^4.2.0",
    "ngx-cookie-service": "^21.1.0",
    "ngx-electron": "^2.2.0",
    "node-machine-id": "^1.1.12",
    "onnxruntime-node": "^1.23.2",
    "primeicons": "^7.0.0",
    "primeng": "^21.0.2",
    "rxjs": "~7.8.0",
    "sharp": "^0.34.5",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.4",
    "@angular/cli": "^21.0.4",
    "@angular/compiler-cli": "^21.0.0",
    "electron": "^39.2.7",
    "electron-builder": "^24.13.3",
    "nodemon": "^3.1.11",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  },
  "build": {
    "appId": "com.vynce.imagesearch",
    "productName": "ISE",
    "directories": {
      "output": "release"
    },
    "files": [
      "main.js",
      "dist/**/*",
      "node_modules/**/*"
    ],
    "extraResources": [
      {
        "from": "resources/assets",
        "to": "resources/assets"
      }
    ],
    "asar": true,
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}